package aula;

public class HipopotamoTest {

	public static void main(String[] args) {
		Animal hipopotamo = new Hipopotamo();

		hipopotamo.setCor("cinza");
		hipopotamo.setPeso(500);

		hipopotamo.comer();
		hipopotamo.andar();
		hipopotamo.dormir();

		System.out.println("O meu hipopotamo pesa " + hipopotamo.getPeso() 
		+ " e Ã© da cor " + hipopotamo.getCor());

	}

}
