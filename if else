package projeto;

public class ifelse {

	public static void main(String[] args) {

		int x;
		x = 20;
		if (x < 10) {
			System.out.println("X é menor que 10");
		} else {
			System.out.println("x é maior que 10");
		}

		int nota = 8;
		if (nota >= 6.0) {
			System.out.println("Aprovado");			
		} else { 
			System.out.println("Reprovado");
		}
	
	}

}
